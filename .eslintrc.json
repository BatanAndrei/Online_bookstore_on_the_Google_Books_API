{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": "error",
        "no-unused-vars": ["off",{ "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
    }
}
